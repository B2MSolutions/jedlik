  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/B2MSolutions/jedlik/raw/master/LICENSE"
    }
  ],
  "devDependencies": {
    "chai": "4.5.0",
    "mocha": "10.8.2",
    "nyc": "17.1.0",
